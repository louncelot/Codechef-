import java.util.Scanner;

class codechef{
    public static void main(String args[]){
        Scanner src=new Scanner(System.in);
        int t=src.nextInt();
        for(int i=0;i<t;i++){
            int a=src.nextInt();
            int b=src.nextInt();
            System.out.println(a+b);
        }
    }
}
